@model IEnumerable<Concrete.Movies>

@{
    ViewBag.Title = "GetMovies";
}

<h2>GetMovies</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReleaseYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductionHouse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImdbRating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CinemaName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReleaseYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductionHouse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImdbRating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CinemaName)
        </td>
        <td>
          @*  @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
